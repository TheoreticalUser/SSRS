  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SSRS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@search">
            <Value>=Parameters!search.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>begin try
	;with src as (
		select
		c.Name as 'REPORT_NAME',
		c.Path as 'REPORT_PATH',
		cast(convert(xml,convert(varbinary(max),c.Content)) as nvarchar(max)) as 'REPORT_XML'
		from  ReportServer.dbo.Catalog c
		where  c.Content is not null
	)
	select REPORT_NAME, REPORT_PATH, REPORT_XML from src
	where REPORT_XML like '%'+@search+'%'
end try
begin catch
	print 'ERROR'
end catch</CommandText>
